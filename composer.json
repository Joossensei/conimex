{
    "name": "bobdenotter/conimex",
    "description": "üçõ Extension to Import and Export content as Yaml files",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "twig/twig": "^2.12 | ^3.0",
        "league/csv": "^9.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "bolt/core": "^4.0 || ^5.0",
        "symplify/easy-coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "BobdenOtter\\Conimex\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "BobdenOtter\\Conimex\\Extension"
    }
}
